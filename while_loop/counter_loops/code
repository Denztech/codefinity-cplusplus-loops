#include <iostream>

double calculateAverage(double transactions[], int size) 
{
    double sum = 0;
    int i = 0;
    while (i < transactions)
        {
            sum = transactions;
            i++;
        }

    return 0;
}

int main()
{
    int size = 5;
    double transactions[5] = {50.0, -20.0, 100.0, -50.0, 30.0};
    double average = calculateAverage(transactions, size);
    std::cout << "The average transaction value is: $" << average;
    return 0;
}